<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="action_report_batch_picking_driver" model="ir.actions.report">
        <field name="name">Batch picking driver</field>
        <field name="model">stock.picking.batch</field>
        <field name="report_type">qweb-pdf</field>
        <field
            name="report_name"
        >stock_picking_batch_report.report_batch_picking_from_move</field>
