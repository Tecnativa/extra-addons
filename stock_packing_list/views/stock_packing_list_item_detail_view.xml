<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="stock_packing_list_item_detail_tree" model="ir.ui.view">
        <field name="name">stock.packing.list.detail.tree</field>
        <field name="model">stock.packing.list.detail</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="stock_move_line_id" optional="hide" />
                <field name="product_id" />
                <field name="lot_id" />
                <field name="qty" />
            </tree>
        </field>
    </record>
    <record id="stock_packing_list_item_detail_form" model="ir.ui.view">
        <field name="name">stock.packing.list.detail.form</field>
        <field name="model">stock.packing.list.detail</field>
        <field name="arch" type="xml">
            <form string="Packing List item">
                <group>
                    <field name="product_id" />
                    <field name="qty" />
                </group>
            </form>
        </field>
    </record>
</odoo>
