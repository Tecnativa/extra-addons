<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="product_gamma_attribute" model="product.attribute">
        <field name="name">Gamma</field>
        <field name="create_variant">no_variant</field>
        <field name="display_type">select</field>
    </record>

    <record id="product_gamma_f" model="product.attribute.value">
        <field name="name">Fresh</field>
        <field name="attribute_id" ref="product_gamma_attribute" />
    </record>

    <record id="product_gamma_c" model="product.attribute.value">
        <field name="name">Frozen</field>
        <field name="attribute_id" ref="product_gamma_attribute" />
    </record>

</odoo>
