        <field
            name="inherit_id"
            ref="stock_picking_product_barcode_report.view_stock_barcode_selection_printing"
        />
        <field name="arch" type="xml">
            <xpath
                expr="//field[@name='product_print_moves']//field[@name='quantity']"
                position="attributes"
            >
                <attribute name="readonly">0</attribute>
            </xpath>
        </field>
    </record>
</odoo>
