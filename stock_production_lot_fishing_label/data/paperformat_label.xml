<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="paperformat_fishing_label" model="report.paperformat">
        <field name="name">Fishing label</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_width">140</field>
        <field name="page_height">90</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">30</field>
        <field name="margin_bottom">10</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">30</field>
        <field name="dpi">200</field>
    </record>

    <record id="paperformat_fishing_label_mini" model="report.paperformat">
        <field name="name">Fishing label mini</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_width">60</field>
        <field name="page_height">45</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">0</field>
        <field name="dpi">100</field>
    </record>
</odoo>
